# sqrt(x) + 1    od 1 do 4

import numpy as np
from matplotlib import pyplot as plt


def f(x):
    return np.sqrt(x) + 1


def pole(a, b, n):
    dx = (b - a) / n
    s = 0
    for i in range(n):
        s += f(a + dx / 2 + i * dx) * dx
    return s


print(pole(1, 4, 10))

x = np.linspace(0, 10, 100)

plt.plot(x, f(x), color='crimson')

plt.show()
